{
    "manifest_version": 3,
    "name": "My React Extension",
    "version": "1.0",
    "description": "A web extension built with React",
    "action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "vite.svg",
        "48": "vite.svg",
        "128": "vite.svg"
      }
    },
    "permissions": [
      "storage",
      "activeTab"
    ]
  }